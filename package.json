{
    "name": "next-dato-tailwind-template",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "rm -rf .next/cache && clear && next dev",
        "build": "next build",
        "start": "next start",
        "serve": "next build && next start --port 8000",
        "lint": "next lint --no-cache",
        "cm": "git add . && cz",
        "nc": "node ./utils/scripts/component.mjs",
        "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,json,scss,css}\"",
        "prepare": "is-ci || husky install"
    },
    "dependencies": {
        "clsx": "^2.0.0",
        "framer-motion": "^10.16.4",
        "husky": "^8.0.3",
        "is-ci": "^3.0.1",
        "next": "13.5.4",
        "next-i18n-router": "^4.1.2",
        "next-themes": "^0.2.1",
        "react": "^18",
        "react-dom": "^18",
        "react-intl": "^6.4.7"
    },
    "devDependencies": {
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "autoprefixer": "^10",
        "eslint": "^8",
        "eslint-config-next": "13.5.4",
        "postcss": "^8",
        "prettier-plugin-tailwindcss": "^0.5.5",
        "tailwindcss": "^3",
        "typescript": "^5"
    }
}
